{
    "name": "nytris/nytris",
    "type": "project",
    "license": "MIT",
    "autoload": {
        "files": [
            "src/bootstrap.php"
        ],
        "psr-4": {
            "Nytris\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Nytris\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=8.1"
    },
    "require-dev": {
        "mockery/mockery": "^1.6",
        "phpstan/phpstan": "^1.10",
        "phpstan/phpstan-mockery": "^1.1",
        "phpunit/phpunit": "^10.2"
    },
    "config": {
        "platform": {
            "php": "8.1"
        },
        "sort-packages": true
    }
}
